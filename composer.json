{
    "name": "gemini",
    "description": "Gemini de expomark",
    "keywords": [
        "microframework",
        "rest",
        "router",
        "psr7"
    ],
    "homepage": "http://github.com/slimphp/Slim-Skeleton",
    "license": "MIT",
    "authors": [
        {
            "name": "Josh Lockhart",
            "email": "info@joshlockhart.com",
            "homepage": "http://www.joshlockhart.com/"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "slim/slim": "^3.1",
        "slim/twig-view": "^2.0",
        "slim/flash": "^0.1.0",
        "monolog/monolog": "^1.17",
        "zeuxisoo/slim-whoops": "0.5.*",
        "slim/csrf": "^0.7.0",
        "illuminate/database": "^5.3",
        "davidepastore/slim-validation": "^0.2.0",
        "willdurand/negotiation": "^2.0",
        "aura/auth": "^2.0",
        "ramsey/uuid": "^3.5",
        "codeguy/upload": "dev-master",
        "cocur/slugify": "^2.3",
        "oscarotero/psr7-middlewares": "^3.18",
        "jasongrimes/paginator": "1.*",
        "twig/extensions": "^1.4",
        "vlucas/phpdotenv": "^2.4"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.8 < 6.0"
